<https://www.fair-gnss.oma.be/gnss-dcat-ap>
  a owl:Ontology, adms:Asset ;
  dct:title "GNSS_DCAT-AP: an extension of the DCAT Application Profile for GNSS observation data"@en ;
  dct:description "The GNSS-DCAT Application aims at providing a commonly-agreed vocabulary for GNSS observation data as Open Data. GNSS-DCAT-AP defines a certain number of additions to the DCAT-AP model that can be used to describe GNSS data e.g. daily RINEX files"@en ;
  dct:created "2022-02-22"^^xsd:date ;
  dct:modified "2022-02-23"^^xsd:date ;
  owl:versionInfo "0.1.0" ;
  vann:preferredNamespacePrefix "gnss" ;
  adms:versionNotes "Change log for GNSS-DCAT-AP v0.1: DCAT-AP v0.1 draft version uploaded as .docx, .pdf and .ttl and aligned with DCAT-AP v.2.1.0"@en ;
  foaf:homepage <https://www.fair-gnss.oma.be/gnss-dcat-ap> ;
  dct:publisher <https://www.fair-gnss.oma.be> ;
  dct:creator <https://www.fair-gnss.oma.be> ;
  dct:type <http://purl.org/adms/assettype/Ontology> ;
  dct:status <http://purl.org/adms/status/UnderDevelopment> .


#---------------------------->
#----- Antenna Class ------>
#---------------------------->

#-- Class definition -->

gnss:Antenna
  a owl:Class ;
  rdfs:comment "Recommended class. Domain specific vocabulary for the antenna associated with gnss:Station"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "Station antenna"@en  .

#-- Properties definition -->

gnss:igsModelCode
  a owl:Property ;
  rdfs:comment "Mandatory property. Antenna and radome name (e.g. TRM57971.00 	NONE)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "Antenna - IGS model code"@en ;
  rdfs:domain gnss:Antenna ;
  rdfs:range rdfs:Literal .

gnss:gnss:marker-arpEastEcc
  a owl:Property ;
  rdfs:comment "Recommended property. Distance from the ARP  to the marker - East component"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "Antenna - marker-ARP East Ecc."@en ;
  rdfs:domain gnss:Antenna ;
  rdfs:range rdfs:Literal .

gnss:gnss:marker-arpNorthEcc
  a owl:Property ;
  rdfs:comment "Recommended property. Distance from the ARP  to the marker - North component"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "Antenna - marker-ARP North Ecc."@en ;
  rdfs:domain gnss:Antenna ;
  rdfs:range xsd:float .

gnss:gnss:marker-arpUpEcc
  a owl:Property ;
  rdfs:comment "Recommended property. Distance from the ARP  to the marker - Up component"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "Antenna - marker-ARP Up Ecc."@en ;
  rdfs:domain gnss:Antenna ;
  rdfs:range xsd:float .

gnss:marker-arpUpEcc
  a owl:Property ;
  rdfs:comment "Recommended property. Antenna and radome name (e.g. TRM57971.00 	NONE)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "Antenna - marker-ARP Up Ecc."@en ;
  rdfs:domain gnss:Antenna ;
  rdfs:range xsd:float .

gnss:manufacturerSerialNumber
  a owl:Property ;
  rdfs:comment "Optional property. Serial number of the antenna"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "Antenna - manufacturer serial number"@en ;
  rdfs:domain gnss:Antenna ;
  rdfs:range rdfs:Literal .


#---------------------------->
#-----  GNSS observation data	Class ------>
#---------------------------->

#-- Class definition -->

gnss:OBSData
  a owl:Class ;
  rdfs:comment "Recommended class. Domain specific vocabulary for RINEX observation files"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS observation data"@en  .

#-- Properties definition -->

gnss:station
  a owl:Property ;
  rdfs:comment "Mandatory property. Station description"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs data - station"@en ;
  rdfs:domain gnss:OBSData ;
  rdfs:range gnss:Station .

gnss:format
  a owl:Property ;
  rdfs:comment "Recommended property. RINEX 2  / RINEX 3/ RINEX 4"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs data - format"@en ;
  rdfs:domain gnss:OBSData ;
  rdfs:range rdfs:Literal .

gnss:obsHeader
  a owl:Property ;
  rdfs:comment "Recommended property. Metadata for the RINEX header"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs data - header"@en ;
  rdfs:domain gnss:OBSData ;
  rdfs:range gnss:OBSHeader .

gnss:frequency
  a owl:Property ;
  rdfs:comment "Optional property. Daily/hourly RINEX files"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs data  - frequency"@en ;
  rdfs:domain gnss:OBSData ;
  rdfs:range dct:Frequency .

gnss:compressFormat
  a owl:Property ;
  rdfs:comment "Optional property. CRX compressed RINEX file (CRX RINEX)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs data  - compression format"@en ;
  rdfs:domain gnss:OBSData ;
  rdfs:range xsd:boolean .

gnss:compressFormatType
  a owl:Property ;
  rdfs:comment "Optional property. Crinex Version (e.g. 3.0)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs data  - compression format type"@en ;
  rdfs:domain gnss:OBSData ;
  rdfs:range rdfs:Literal .


#---------------------------->
#-----  GNSS observation data header Class ------>
#---------------------------->

#-- Class definition -->

gnss:OBSHeader
  a owl:Class ;
  rdfs:comment "Recommended class. Domain specific vocabulary for the information in the RINEX observation file header associated with gnss:OBSData"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS observation data header"@en  .

#-- Properties definition -->

gnss:antenna
  a owl:Property ;
  rdfs:comment "Recommended property. Antenna information from the RINEX header"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - antenna"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range gnss:Antenna .

gnss:interval
  a owl:Property ;
  rdfs:comment "Recommended property. 30 sec / 15 sec/1 sec"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - interval"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range dct:Frequency .

gnss:receiver
  a owl:Property ;
  rdfs:comment "Recommended property. Receiver information from the RINEX header"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - receiver as in OBSHeader"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range gnss:Receiver .

gnss:issued
  a owl:Property ;
  rdfs:comment "Recommended property. Publication date of the header"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - issued date"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range xsd:date .

gnss:temporal
  a owl:Property ;
  rdfs:comment "Recommended property. Temporal coverage from the RINEX header (start time/date-end time/date)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - temporal coverage"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range dct:PeriodOfTime .

gnss:agency
  a owl:Property ;
  rdfs:comment "Optional property. Agency as in the RINEX header (e.g., ROB)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - agency"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range rdfs:Literal .

gnss:isVersionOf
  a owl:Property ;
  rdfs:comment "Optional property. Related gnss:OBSHeader of which the described header is a version, edition or adaptation"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - version of GNSS obs header"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range gnss:OBSHeader .

gnss:markerName
  a owl:Property ;
  rdfs:comment "Optional property. Marker name in the RINEX header (4-char/9-char id)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header -  marker name"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range rdfs:Literal .

gnss:markerNumber
  a owl:Property ;
  rdfs:comment "Optional property. DOMES number"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - marker number"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range rdfs:Literal .

gnss:observer
  a owl:Property ;
  rdfs:comment "Optional property. Observer as in the RINEX header (e.g., John Smith)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - observer"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range rdfs:Literal .

gnss:pgm
  a owl:Property ;
  rdfs:comment "Optional property. Info regarding the software used to generate the file as from the RINEX header"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - pgm"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range gnss:PGM .

gnss:rinexVersion
  a owl:Property ;
  rdfs:comment "Optional property. 	RINEX sub-version (text e.g 3.04)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS obs header - RINEX sub-version"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range rdfs:Literal .

owl:versionInfo
  a owl:Property ;
  rdfs:comment "Optional property. Version number or other version designation of the RINEX Header"@en  ;
  rdfs:isDefinedBy <http://www.w3.org/2002/07/owl#versionInfo> ;
  rdfs:label "GNSS obs header - version info"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range rdfs:Literal .

adms:versionNotes
  a owl:Property ;
  rdfs:comment "Optional property. Description of the differences between current and a previous version of the RINEX header"@en  ;
  rdfs:isDefinedBy <http://www.w3.org/ns/adms#versioNotes> ;
  rdfs:label "GNSS obs header - version notes"@en ;
  rdfs:domain gnss:OBSHeader ;
  rdfs:range rdfs:Literal .


#---------------------------->
#-----  GNSS observation data generating software	Class ------>
#---------------------------->

#-- Class definition -->

gnss:PGM
  a owl:Class ;
  rdfs:comment "Recommended class. Domain specific vocabulary for software used to generate the RINEX file associated with gnss:OBSData"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "GNSS observation data generating software"@en  .

#-- Properties definition -->

gnss:pgmName
  a owl:Property ;
  rdfs:comment "Mandatory property. Name of the software used to generate the RINEX file (e.g. Spider V7.1.1.7438 )"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "PGM - name"@en ;
  rdfs:domain gnss:PGM ;
  rdfs:range rdfs:Literal .

gnss:pgmNotes
  a owl:Property ;
  rdfs:comment "Optional property. Comment (e.g. THIS RINEX FILE IS CREATED FROM LB2 DATA)"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "PGM - notes"@en ;
  rdfs:domain gnss:PGM ;
  rdfs:range rdfs:Literal .

gnss:pgmDate
  a owl:Property ;
  rdfs:comment "Optional property. PGM generation date as from the RINEX header"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "PGM - date"@en ;
  rdfs:domain gnss:PGM ;
  rdfs:range xsd:date .

gnss:pgmRunBy
  a owl:Property ;
  rdfs:comment "Optional property. Name of the agency operating the software generating the RINEX file"@en  ;
  rdfs:isDefinedBy <https://www.fair-gnss.oma.be/gnss-dcat-ap/vocabulary/gnss#> ;
  rdfs:label "PGM - operated by"@en ;
  rdfs:domain gnss:PGM ;
  rdfs:range rdfs:Literal .


